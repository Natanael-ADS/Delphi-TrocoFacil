unit S_TrocoFacil;

interface
uses
  B_Cliente,B_Empresa,B_Atendente;

type
  TSTrocoFacil = class
    private
    public
      function Cadastrar(BCliente: TBCliente): string;
      function DevolverTroco(BAtendente: TBAtendente): string;
      function ReceberPagamento(BAtendente: TBAtendente):string;
      constructor Create();
      destructor Destroy();
  end;

implementation

end.
